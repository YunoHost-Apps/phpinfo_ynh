                {
    "name": "Phpinfo",
    "id": "phpinfo",
    "packaging_format": 1,
    "description": {
        "en": "Simple phpinfo",
        "fr": "Simple phpinfo"
    },
    "version": "1.0~ynh2",
    "url": "https://www.php.net/manual/fr/function.phpinfo.php",
    "license": "GPL-3.0-or-later",
    "maintainer": {
		"name": "Maniack Crudelis",
		"email": "maniackc_dev@crudelis.fr"
    },
    "requirements": {
        "yunohost": ">= 4.2.8"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/phpinfo",
                "default": "/phpinfo"
            },
            {
                "name": "phpversion",
                "type": "string",
                "ask": {
                    "en": "Choose the version of PHP to use.",
                    "fr": "Choisissez la version de PHP Ã  utiliser."
                },
                "choices": ["7.3",, "7.4", "8.0"],
                "default": "7.3"
            }
        ]
    }
}
